FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalField   uniform (0 0 0);

boundaryField
{
    wall-4
    {
        type            generalFixedValue;
        value           (0 0 0);
    }

    velocity-inlet-5
    {
        type            generalFixedValue;
        value           (1 0 0);
    }

    velocity-inlet-6
    {
        type            generalFixedValue;
        value           (0 3 0);
    }

    pressure-outlet-7
    {
        type            generalZeroGradient;
    }

    wall-8
    {
        type            generalFixedValue;
        value           (0 0 0);
    }

    frontAndBackPlanes
    {
        type            generalFixedValue;
        value           (0 0 0);
    }
}


