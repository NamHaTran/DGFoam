FoamFile
{
	version  2.0;
    format  ascii;
    class  volScalarField;
    location "0";
    object  rho_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 0 0 0 0 0 0];
internalField   uniform 2.8e-5;

boundaryField
{
    inlet
    {
        type            calculated;
	value           uniform 2.8e-5;
    }
    outlet2
    {
        type            calculated;
	value           uniform 2.8e-5;
    }
    outlet1
    {
        type            calculated;
	value           uniform 2.8e-5;
    }
    wall
    {
        type            calculated;
	value           uniform 2.8e-5;
    }
    symmetryBottom
    {
        type            symmetry;
    }
    symmetryZ0
    {
        type            symmetry;
    }
    symmetryZ05
    {
        type            symmetry;
    }
}


