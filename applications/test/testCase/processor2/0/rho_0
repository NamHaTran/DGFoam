FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      rho_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2.8e-05;

boundaryField
{
    inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    outlet2
    {
        type            calculated;
        value           uniform 2.8e-05;
    }
    outlet1
    {
        type            calculated;
        value           uniform 2.8e-05;
    }
    wall
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    symmetryBottom
    {
        type            symmetry;
    }
    symmetryZ0
    {
        type            symmetry;
    }
    symmetryZ05
    {
        type            symmetry;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 2.8e-05;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 2.8e-05;
    }
}


